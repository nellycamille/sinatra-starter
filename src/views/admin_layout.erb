<!doctype html>
<html>
  <head>
      <meta charset="utf-8"></meta>
    <title>Photocabinet</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.3/normalize.css" media="screen" charset="utf-8">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="css/pgwslideshow.css" media="screen" title="no title" charset="utf-8">
    <link href='https://fonts.googleapis.com/css?family=Montserrat:700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed:300' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=EB+Garamond' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Indie+Flower' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="js/main.js"></script>
    <script src="js/pgwslideshow.js"></script>

    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>

    <script src="/js/select-auto-refresh.js"></script>
    <link rel='stylesheet' href='/css/style.css' media="screen" title="no title" charset="utf-8"></link>

  </head>
  <body>

    <div class="navbar-static-top" class="cssmenu">
          <div class="container-fluid">
            <div class="row nav">
              <div class="col-lg-12">
                  <div id='cssmenu'>
                   <ul id="desktop">
                     <li><a href="/"><img src="/img/logo.png" alt="logo" height="100px" width="130px"/></a></li>
                    <li><a href="/admin/leads">Manage Leads</a></li>
                    <li><a href="/admin/uploads">Manage Galleries</a></li>
                    <li><a href="/logout">Logout</a></li>
                  </ul>
                  <ul id="mobile">
                  <li><a class="link-home"><img src="/img/logo.png" alt="logo" height="100px" width="130px"/></a></li>
                  <li><a href="/admin/leads">Manage Leads</a></li>
                  <li><a href="/admin/uploads">Manage Galleries</a></li>
                  <li><a href="/logout">Logout</a></li>
                 </ul>
                </div>
              </div>
            </div>
          </div>
        </div>

      <!-- MAIN CONTENT -->
      <div class='container'>

        <% flash.each do | key , value | %>
          <div class='alert alert-<%= key %>'>
              <%= value %>
          </div>
        <% end %>

        <%= yield %>
      </div>

      <!-- MAIN CONTENT -->
      <div class="container-fluid">
      <div class="row">
        <div class="main-section row main-resume">
          <div class="navbar col-sm-12">
            <hr style="width: 90%;"></hr>
          <h3 class="footer">&#x24B8; Copyright 2015 Photocabinet, Puerto Rico</h3>
          </div>
        </div>
      </div>
      </div>
  </body>
</html>
